@page
@model Serwis.Pages.SavedModel
@{
    ViewData["Title"] = "Saved";
}

<h1 class="display-4">Saved</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.People[0].FullName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.People[0].Rok)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.People[0].Date)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.People) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rok)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
        </tr>
}
    </tbody>
</table>